<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.tintuc"
    android:background="@drawable/tuybiennut3">

    <RelativeLayout
        android:id="@+id/rs"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/tuybn4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imgtt"
            android:layout_width="98dp"
            android:layout_height="104dp"
            android:layout_below="@+id/imgttt"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="24dp"
            android:scaleType="fitXY"
            tools:srcCompat="@tools:sample/avatars" />

        <TextView
            android:id="@+id/txtaqi"
            android:layout_width="89dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="19dp"
            android:layout_marginTop="24dp"
            android:textStyle="italic|bold"
            android:fontFamily="@font/s"
            android:layout_toEndOf="@+id/imgttt" />

        <TextView
            android:id="@+id/txtvt"
            android:layout_width="117dp"
            android:layout_height="125dp"
            android:layout_alignParentTop="true"
            android:layout_marginStart="5dp"
            android:textStyle="italic|bold"
            android:fontFamily="@font/s"
            android:layout_marginTop="24dp"
            android:layout_toEndOf="@+id/txtaqi" />

        <TextView
            android:id="@+id/txtndtt"
            android:layout_width="220dp"
            android:layout_height="62dp"
            android:layout_below="@+id/imgtt"
            android:layout_marginStart="8dp"
            android:textStyle="italic|bold"
            android:layout_marginTop="-64dp"
            android:fontFamily="@font/s"
            android:layout_toEndOf="@+id/imgtt" />

        <TextView
            android:id="@+id/txtdamtt"
            android:layout_width="124dp"
            android:layout_height="35dp"
            android:layout_below="@+id/txtaqi"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/s"
            android:textStyle="italic|bold"
            android:layout_marginTop="109dp"
            android:layout_toEndOf="@+id/imgtt" />

        <TextView
            android:id="@+id/txtgio"
            android:layout_width="90dp"
            android:layout_height="35dp"
            android:layout_below="@+id/txtaqi"
            android:layout_marginStart="6dp"
            android:fontFamily="@font/s"
            android:textStyle="italic|bold"
            android:layout_marginTop="109dp"
            android:layout_toEndOf="@+id/txtdamtt" />

        <TextView
            android:id="@+id/txtthongtin"
            android:layout_width="323dp"
            android:layout_height="147dp"
            android:fontFamily="@font/s"
            android:textStyle="italic|bold"
            android:layout_below="@+id/txtndtt"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="44dp"
            android:layout_marginEnd="10dp"
            android:background="#333333"
            android:textColor="#FFFACD" />

        <ImageView
            android:id="@+id/imgttt"
            android:layout_width="98dp"
            android:layout_height="104dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="11dp"
            android:layout_marginTop="24dp"
            android:background="@color/black"
            tools:srcCompat="@tools:sample/avatars" />

        <Button
            android:id="@+id/btnl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/spn_tinh"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="12dp"
            android:fontFamily="@font/s"
            android:background="@drawable/btn_login"
            android:text="Reload"
            android:textColor="#2B0000"
            android:textStyle="bold|italic" />

        <Spinner
            android:id="@+id/sp_ds"
            android:layout_width="241dp"
            android:layout_height="48dp"
            android:textStyle="italic|bold"
            android:layout_below="@+id/spn_tinh"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/s"
            android:background="#ffffff"
            tools:ignore="SpeakableTextPresentCheck" />

        <Spinner
            android:id="@+id/spn_tinh"
            android:layout_width="267dp"
            android:layout_height="48dp"
            android:textStyle="italic|bold"
            android:layout_below="@+id/txtthongtin"
            android:layout_alignParentStart="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="21dp"
            android:fontFamily="@font/s"
            android:background="#ffffff"
            tools:ignore="SpeakableTextPresentCheck" />

        <Spinner
            android:id="@+id/spn_sv"
            android:layout_width="58dp"
            android:layout_height="48dp"
            android:textStyle="italic|bold"
            android:layout_below="@+id/txtthongtin"
            android:layout_marginStart="4dp"
            android:layout_marginTop="21dp"
            android:fontFamily="@font/s"
            android:layout_toEndOf="@+id/spn_tinh"
            android:background="#ffffff"
            tools:ignore="SpeakableTextPresentCheck" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>