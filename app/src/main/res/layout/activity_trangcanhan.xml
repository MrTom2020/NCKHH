<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".trangcanhan"
        android:background="@drawable/tuybiennut3">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="836dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/tuybn4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.529"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="334dp"
                android:layout_height="58dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="39dp"
                android:layout_marginTop="51dp"
                android:layout_marginEnd="39dp"
                android:fontFamily="@font/s"
                android:gravity="center"
                android:text="Individual"
                android:textColor="#FFFACD"
                android:textSize="30dp"
                android:textStyle="italic|bold" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="103dp"
                android:layout_height="23dp"
                android:fontFamily="@font/s"
                android:layout_below="@+id/textView7"
                android:layout_alignParentStart="true"
                android:layout_marginStart="33dp"
                android:layout_marginTop="59dp"
                android:text="Name"
                android:textColor="#FFFACD" />

            <EditText
                android:id="@+id/edttencanhan"
                android:layout_width="211dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView7"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="45dp"
                android:fontFamily="@font/s"
                android:layout_marginEnd="19dp"
                android:background="@drawable/tuybiennut"
                android:ems="10"
                android:textStyle="italic|bold"
                android:enabled="false"
                android:hint="........."
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textColor="#2b0000"
                android:textColorHint="#2b0000" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="103dp"
                android:layout_height="23dp"
                android:textStyle="italic|bold"
                android:fontFamily="@font/s"
                android:layout_below="@+id/textView8"
                android:layout_alignParentStart="true"
                android:layout_marginStart="33dp"
                android:layout_marginTop="65dp"
                android:text="Password"
                android:textColor="#FFFACD" />

            <EditText
                android:id="@+id/edtmkcanhan"
                android:layout_width="211dp"
                android:layout_height="48dp"
                android:textStyle="italic|bold"
                android:layout_below="@+id/edttencanhan"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="45dp"
                android:layout_marginEnd="19dp"
                android:background="@drawable/tuybiennut"
                android:ems="10"
                android:fontFamily="@font/s"
                android:hint="......."
                android:inputType="textPersonName"
                android:textColor="#2b0000"
                android:textColorHint="#2b0000" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="103dp"
                android:layout_height="23dp"
                android:textStyle="italic|bold"
                android:fontFamily="@font/s"
                android:layout_below="@+id/edtmkcanhan"
                android:layout_alignParentStart="true"
                android:layout_marginStart="33dp"
                android:layout_marginTop="49dp"
                android:text="Address"
                android:textColor="#FFFACD" />

            <EditText
                android:id="@+id/edtdiachicanhan"
                android:layout_width="211dp"
                android:layout_height="wrap_content"
                android:textStyle="italic|bold"
                android:fontFamily="@font/s"
                android:layout_below="@+id/edtmkcanhan"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="19dp"
                android:background="@drawable/tuybiennut"
                android:ems="10"
                android:hint="......."
                android:minHeight="48dp"
                android:textColor="#2b0000"
                android:textColorHint="#2b0000"
                tools:ignore="SpeakableTextPresentCheck" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="103dp"
                android:layout_height="23dp"
                android:textStyle="italic|bold"
                android:fontFamily="@font/s"
                android:layout_below="@+id/edtdiachicanhan"
                android:layout_alignParentStart="true"
                android:layout_marginStart="33dp"
                android:layout_marginTop="49dp"
                android:text="Birthday"
                android:textColor="#FFFACD" />

            <EditText
                android:id="@+id/edtngaysinhcanhan"
                android:layout_width="211dp"
                android:textStyle="italic|bold"
                android:layout_height="wrap_content"
                android:layout_below="@+id/edtdiachicanhan"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="36dp"
                android:fontFamily="@font/s"
                android:layout_marginEnd="19dp"
                android:background="@drawable/tuybiennut"
                android:ems="10"
                android:hint="......"
                android:inputType="datetime"
                android:minHeight="48dp"
                android:textColor="#2b0000"
                android:textColorHint="#2b0000"
                tools:ignore="SpeakableTextPresentCheck" />

            <Button
                android:id="@+id/btndycn"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_below="@+id/txtkey"
                android:fontFamily="@font/s"
                android:textStyle="italic|bold"
                android:layout_alignParentStart="true"
                android:layout_marginStart="72dp"
                android:layout_marginTop="39dp"
                android:background="@drawable/btn_login"
                android:text="Exit"
                android:textColor="#2B0000" />

            <Button
                android:id="@+id/btnluucanhan"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:textStyle="italic|bold"
                android:layout_below="@+id/txtkey"
                android:layout_marginStart="71dp"
                android:layout_marginTop="39dp"
                android:fontFamily="@font/s"
                android:layout_toEndOf="@+id/btndycn"
                android:background="@drawable/btn_login"
                android:text="Save"
                android:textColor="#2B0000" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView12"
                android:layout_alignParentStart="true"
                android:layout_marginStart="33dp"
                android:textStyle="italic|bold"
                android:fontFamily="@font/s"
                android:layout_marginTop="65dp"
                android:text="Phone number"
                android:textColor="#FFFACD" />

            <EditText
                android:id="@+id/edt_sdtcn"
                android:layout_width="211dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/edtngaysinhcanhan"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="19dp"
                android:textStyle="italic|bold"
                android:fontFamily="@font/s"
                android:background="@drawable/tuybiennut"
                android:ems="10"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="......"
                android:textColor="#2b0000" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView14"
                android:layout_alignParentStart="true"
                android:layout_marginStart="33dp"
                android:layout_marginTop="65dp"
                android:fontFamily="@font/s"
                android:text="Key chain"
                android:textColor="#FFFACD"
                android:textStyle="italic|bold" />

            <TextView
                android:id="@+id/txtkey"
                android:layout_width="211dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/s"
                android:layout_below="@+id/edt_sdtcn"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="36dp"
                android:textStyle="italic|bold"
                android:layout_marginEnd="19dp"
                android:background="@drawable/tuybiennut"
                android:text="........"
                android:textColor="#2b0000" />

        </RelativeLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>